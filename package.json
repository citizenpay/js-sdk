{
    "name": "@citizenpay/sdk",
    "version": "1.0.4",
    "description": "An sdk to easily work with citizen pay.",
    "main": "./dist/src/index.js",
    "module": "./dist/src/index.js",
    "types": "./dist/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/citizenpay/js-sdk.git"
    },
    "bugs": {
        "url": "https://github.com/citizenpay/js-sdk/issues"
    },
    "scripts": {
        "build": "rimraf dist && tsc",
        "watch": "tsc-watch --onSuccess \"tsc\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "manual-publish": "npm publish --access public"
    },
    "keywords": [
        "web3",
        "sdk",
        "4337",
        "erc4337",
        "account abstraction",
        "wallet",
        "tokens",
        "erc20",
        "userop",
        "evm",
        "citizen pay"
    ],
    "author": "Kevin Sundar Raj",
    "license": "MIT",
    "files": [
        "package.json",
        "LICENSE",
        "README.md",
        "dist",
        "src",
        "env.example",
        "tsconfig.json",
        ".gitignore"
    ],
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.16",
        "@types/react": "^18.2.57",
        "fs": "^0.0.1-security",
        "jest": "^29.7.0",
        "nock": "^13.5.1",
        "path": "^0.12.7",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "tsc-watch": "^6.0.4",
        "tsx": "^4.19.2"
    },
    "dependencies": {
        "ethers": "^6.11.1",
        "fflate": "^0.8.2",
        "react": "^18.2.0",
        "typescript": "^5.6.3"
    },
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/src/index.js",
            "require": "./dist/src/index.js"
        }
    }
}
